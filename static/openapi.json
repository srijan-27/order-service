        ]
      },
      "AzurePostRequest": {
        "type": "object",
        "properties": {
          "resourceGroup": {
            "type": "string",
            "description": "Name of the Azure resource group",
            "example": "test"
          },
          "clusterName": {
            "type": "string",
            "description": "Name of the Kubernetes cluster",
            "example": "test-aws-2305"
          },
          "namespaceName": {
            "type": "string",
            "description": "Name of the namespace",
            "example": "test-app"
          },
          "cloudPlatform": {
            "type": "string",
            "description": "Name of the cloud platform",
            "example": "azure"
          },
          "appRegion": {
            "type": "string",
            "description": "Region of the application",
            "example": "us-east-2"
          },
          "credential": {
            "type": "object",
            "properties": {
              "subscriptionId": {
                "type": "string",
                "description": "Azure subscription ID",
                "example": "263f0185-28e5-44b4-95a0-e318720493ac"
              },
              "tenantId": {
                "type": "string",
                "description": "Azure tenant ID",
                "example": "312a4e75-d8b6-45e3-b300-841b57a4a219"
              },
              "appId": {
                "type": "string",
                "description": "Azure application (client) ID",
                "example": "27a25248-856e-4b62-b48a-15ffc9203232"
              },
              "password": {
                "type": "string",
                "description": "Azure application password",
                "example": "xuE8Q~fasf"
              }
            },
            "required": [
              "subscriptionId",
              "tenantId",
              "appId",
              "password"
            ]
          }
        },
        "required": [
          "resourceGroup",
          "clusterName",
          "namespaceName",
          "cloudPlatform",
          "appRegion",
          "credential"
        ]
      },
      "LogEntry": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "example": "2024-05-23T12:38:22.324285764Z"
          },
          "message": {
            "type": "string",
            "example": "{\"level\":\"INFO\",\"time\":\"2024-05-23T12:38:22.324084567Z\",\"message\":\"tracing is enabled on: https://tempo-distributor.observability-stg.343328ae-79a9-42b1-be89-bbb227b41de6.zscloud.zopsmart.com\",\"system\":{\"alloc\":7237256,\"dataCenter\":\"NOTSET\",\"goRoutines\":3,\"host\":\"hello-service-86d98d7966-tcvkx\",\"numGC\":2,\"sys\":19743752,\"totalAlloc\":8977280},\"app\":{\"name\":\"hello-service\",\"version\":\"v1\",\"framework\":\"gofr-v0.4.0\"}}"
          }
        }
      },
      "LogResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LogEntry"
            }
          }
        }
      }
